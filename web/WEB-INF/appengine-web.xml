<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <!--  <application>ukraina-centr</application>-->
    <application>our-little-baby</application>
    <version>1</version>
    <threadsafe>true</threadsafe>
    <runtime>java8</runtime>
    <sessions-enabled>true</sessions-enabled>
    <!--setting below allows to issue outbound http(s) requests without enabling billing -->
    <url-stream-handler>urlfetch</url-stream-handler>

    <resource-files>
        <include path="/WEB-INF/templates/**.soy" />
    </resource-files>
</appengine-web-app>
